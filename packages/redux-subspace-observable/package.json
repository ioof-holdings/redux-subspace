{
  "name": "redux-subspace-observable",
  "version": "2.3.1",
  "description": "redux-subspace wrappers for redux-observable",
  "author": "Michael Peyper",
  "contributors": [
    "Jonathan Peyper",
    "Vivian Farrell"
  ],
  "license": "BSD-3-Clause",
  "main": "lib/index.js",
  "module": "es/index.js",
  "typings": "src/index.d.ts",
  "sideEffects": false,
  "files": ["lib", "es", "src"],
  "scripts": {
    "predist": "rimraf lib es",
    "dist": "rollup -c",
    "lint": "eslint . --ext .js --ext .jsx",
    "lint:fix": "eslint . --ext .js --ext .jsx --fix",
    "test": "cross-env NODE_ENV=test nyc mocha --compilers js:babel-register --recursive --require ./test/setup.js",
    "test:watch": "npm test -- --watch",
    "prepublish": "npm run dist"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/ioof-holdings/redux-subspace.git"
  },
  "dependencies": {
    "@types/es6-shim": "^0.31.34",
    "@types/node": "^8.0.47",
    "redux-subspace": "^2.3.1",
    "rxjs": "^5.5.2"
  },
  "peerDependencies": {
    "redux": "^3.0.0",
    "redux-observable": "^0.16.0"
  },
  "devDependencies": {
    "babel-cli": "^6.18.0",
    "babel-core": " ^6.7.7",
    "babel-eslint": "^8.0.1",
    "babel-plugin-external-helpers": "^6.22.0",
    "babel-plugin-transform-function-bind": "^6.22.0",
    "babel-preset-env": "^1.6.1",
    "babel-preset-react": "^6.5.0",
    "babel-preset-stage-3": "^6.24.1",
    "babel-register": "^6.7.2",
    "chai": "^4.1.2",
    "cross-env": "^5.1.3",
    "eslint": "^4.10.0",
    "eslint-plugin-react": "^7.4.0",
    "mocha": "^4.0.1",
    "nyc": "^11.3.0",
    "redux": "^3.6.0",
    "redux-mock-store": "^1.3.0",
    "redux-observable": "^0.17.0",
    "rollup": "^0.55.5",
    "rollup-plugin-babel": "^3.0.3",
    "sinon": "^4.0.2",
    "sinon-chai": "^2.14.0",
    "typescript": "^2.5.3",
    "typescript-definition-tester": "0.0.5"
  },
  "nyc": {
    "temp-directory": "coverage/.nyc_output",
    "reporter": [
      "html",
      "text",
      "json"
    ]
  }
}
