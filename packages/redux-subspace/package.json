{
  "name": "redux-subspace",
  "version": "3.0.1",
  "description": "Create isolated subspaces of a Redux store",
  "author": "Michael Peyper",
  "contributors": [
    "Jonathan Peyper",
    "Vivian Farrell"
  ],
  "license": "BSD-3-Clause",
  "main": "lib/index.js",
  "module": "es/index.js",
  "typings": "src/index.d.ts",
  "sideEffects": false,
  "files": [
    "lib",
    "es",
    "src"
  ],
  "scripts": {
    "predist": "rimraf lib es",
    "dist": "rollup -c",
    "lint": "eslint . --ext .js --ext .jsx",
    "lint:fix": "eslint . --ext .js --ext .jsx --fix",
    "test": "cross-env NODE_ENV=test nyc mocha --compilers js:@babel/register --require ./test/setup.js $(find test -name '*-spec.js')",
    "test:watch": "npm test -- --watch",
    "prepublish": "npm run dist"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/ioof-holdings/redux-subspace.git"
  },
  "peerDependencies": {
    "redux": "^3.0.0 || ^4.0.0"
  },
  "devDependencies": {
    "@babel/core": "^7.3.3",
    "@babel/plugin-proposal-object-rest-spread": "^7.3.2",
    "@babel/plugin-transform-modules-commonjs": "^7.1.0",
    "@babel/preset-env": "^7.3.1",
    "@babel/register": "^7.0.0",
    "babel-eslint": "^10.0.1",
    "babel-plugin-annotate-pure-calls": "^0.4.0",
    "chai": "^4.2.0",
    "cross-env": "^5.1.3",
    "eslint": "^5.14.1",
    "eslint-plugin-react": "^7.12.4",
    "mocha": "^5.2.0",
    "nyc": "^13.3.0",
    "redux": "^4.0.1",
    "redux-mock-store": "^1.3.0",
    "redux-promise": "^0.6.0",
    "redux-thunk": "^2.2.0",
    "rollup": "^1.2.1",
    "rollup-plugin-babel": "^4.3.2",
    "sinon": "^7.2.4",
    "sinon-chai": "^3.2.0",
    "typescript": "^3.3.3",
    "typescript-definition-tester": "0.0.6"
  },
  "nyc": {
    "temp-directory": "coverage/.nyc_output",
    "reporter": [
      "html",
      "text",
      "json"
    ]
  }
}
